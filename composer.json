{
    "name": "ledevoir/pianoid-api-php-sdk",
    "description": "The unofficial PHP SDK for PianoID API",
    "type": "library",
    "license": [
        "MIT"
    ],
    "authors": [
        {
            "name": "Philippe bourdeau",
            "email": "philippe@cloudhelp.ca"
        }
    ],
    "require": {
        "php": ">=8.2",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7"
    },
    "autoload": {
        "psr-4" :{
            "LeDevoir\\PianoIdApiSDK\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LeDevoir\\PianoIdApiSDK\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10"
    }
}
